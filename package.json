{
  "name": "crud-javascript",
  "version": "1.2.2",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "scripts": {
    "start": "parcel src/index.html -d docs/",
    "serve": "parcel serve src/index.html -d docs/",
    "watch": "parcel watch src/index.html -d docs/",
    "build": "parcel build src/index.html -d docs/ --public-url ./"
  },
  "dependencies": {
    "Promise": "^1.0.5",
    "admin-lte": "~3.0.5"
  },
  "devDependencies": {
    "@lacussoft/eslint-config": "^1.0.1",
    "eslint": "^7.16.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.30.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "browser": true
    },
    "extends": [
      "@lacussoft"
    ],
    "ignorePatterns": [
      "docs/",
      "public/"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": "off"
    }
  }
}
