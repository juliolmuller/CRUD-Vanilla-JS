{
  "name": "crud-javascript",
  "version": "1.2.1",
  "license": "MIT",
  "author": {
    "name": "Julio L. M",
    "url": "https://juliolmuller.github.io/"
  },
  "scripts": {
    "start": "parcel src/index.html -d docs/",
    "serve": "parcel serve src/index.html -d docs/",
    "watch": "parcel watch src/index.html -d docs/",
    "build": "parcel build src/index.html -d docs/ --public-url ./"
  },
  "dependencies": {
    "Promise": "^1.0.5",
    "admin-lte": "~3.0.5"
  },
  "devDependencies": {
    "@lacussoft/eslint-config-standard": "^1.0.9",
    "eslint": "^7.12.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.27.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "@lacussoft/standard",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "rules": {
      "no-underscore-dangle": "off"
    }
  }
}
